{
  "name": "apptuner",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "watcher": "node watcher-server.cjs",
    "metro": "node metro-server.cjs",
    "relay": "cd relay && npm run dev",
    "mobile": "cd mobile && npm start",
    "start:all": "concurrently -n desktop,relay,watcher,metro,mobile -c blue,green,yellow,cyan,magenta \"npm run dev\" \"npm run relay\" \"npm run watcher\" \"npm run metro\" \"npm run mobile\""
  },
  "dependencies": {
    "@react-native/metro-config": "^0.83.1",
    "@stripe/stripe-js": "^8.7.0",
    "@supabase/supabase-js": "^2.95.3",
    "@tauri-apps/api": "^2.0.0",
    "@tauri-apps/plugin-dialog": "^2.0.0",
    "@tauri-apps/plugin-fs": "^2.0.0",
    "chokidar": "^5.0.0",
    "metro": "^0.83.3",
    "metro-config": "^0.83.3",
    "metro-resolver": "^0.83.3",
    "metro-runtime": "^0.83.3",
    "metro-transform-worker": "^0.83.3",
    "qrcode": "^1.5.4",
    "qrcode.react": "^4.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@tauri-apps/cli": "^2.0.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.2.1",
    "esbuild": "^0.27.2",
    "typescript": "^5.5.0",
    "vite": "^5.4.0"
  }
}
